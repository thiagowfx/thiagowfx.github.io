{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "categories": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "feeds": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "title": {
                  "type": "string"
                },
                "url": {
                  "type": "string",
                  "format": "uri"
                },
                "feed": {
                  "type": "string",
                  "format": "uri"
                }
              },
              "required": [
                "title",
                "url",
                "feed"
              ],
              "additionalProperties": false
            }
          }
        },
        "required": [
          "name",
          "feeds"
        ],
        "additionalProperties": false
      }
    }
  },
  "required": [
    "categories"
  ],
  "additionalProperties": false
}
